# Google Gemini API Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
# Gemini API is used as fallback if Google Translate fails
GENAI_API_KEY=your_gemini_api_key_here
# Alternative variable name (either works)
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Specify Gemini model (default: models/gemini-2.0-flash)
GENAI_MODEL=models/gemini-2.0-flash
# Alternative variable name
GEMINI_MODEL=models/gemini-2.0-flash

# Note: Google Translate library is used as the primary translator for PDF translation
# No API key required - it's a free Python library

# Streamlit Configuration
# Disable analytics/telemetry (prevents data forwarding to Fivetran)
# Alternative to .streamlit/config.toml - set this environment variable
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false

